{
  "name": "shubham-portfolio",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "main": "src/index.tsx",
  "dependencies": {
    "autoprefixer": "10.2.5",
    "cross-env": "7.0.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.2.14",
    "postcss-cli": "8.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.0",
    "tailwindcss": "2.1.2"
  },
  "devDependencies": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "typescript": "4.1.3",
    "react-jsx": "1.0.0"
  },
  "scripts": {
    "build:css": "cross-env TAILWIND_MODE=build NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/index.css",
    "watch:css": "cross-env TAILWIND_MODE=watch NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/index.css --watch",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "start": "run-p env:dev DISABLE_NEW_JSX_TRANSFORM=true react-scripts start",
    "build": "run-s env:prod DISABLE_NEW_JSX_TRANSFORM=true react-scripts build",
    "test": "DISABLE_NEW_JSX_TRANSFORM=true react-scripts test --env=jsdom",
    "eject": "DISABLE_NEW_JSX_TRANSFORM=true react-scripts reject"
  },
  "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"]
}
